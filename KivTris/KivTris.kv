<Menu5>:
    name: "Highscore"
    ## Highscore
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.width, self.height/2
            source: "KivTris_back2_dark.png"

    BoxLayout:
        padding: 10
        spacing: 20
        orientation: 'vertical'
        size: self.parent.size
        pos: self.parent.pos

        BoxLayout:
            size_hint: 1., .6
            orientation: 'vertical'
            center: root.center

            Label:
                text: "Highscore"
                font_size: 24

            HighscoreLbl1:
                text: " "
            HighscoreLbl1:
                text: root.highscoreText[ 0]
            HighscoreLbl2:
                text: root.highscoreText[ 1]
            HighscoreLbl1:
                text: root.highscoreText[ 2]
            HighscoreLbl2:
                text: root.highscoreText[ 3]
            HighscoreLbl1:
                text: root.highscoreText[ 4]
            HighscoreLbl2:
                text: root.highscoreText[ 5]
            HighscoreLbl1:
                text: root.highscoreText[ 6]
            HighscoreLbl2:
                text: root.highscoreText[ 7]
            HighscoreLbl1:
                text: root.highscoreText[ 8]
            HighscoreLbl2:
                text: root.highscoreText[ 9]
            HighscoreLbl1:
                text: root.highscoreText[10]
            HighscoreLbl2:
                text: root.highscoreText[11]
            HighscoreLbl1:
                text: root.highscoreText[12]
            HighscoreLbl2:
                text: root.highscoreText[13]
            HighscoreLbl1:
                text: root.highscoreText[14]


        BoxLayout:
            padding: 30
            spacing: 10
            size_hint: 1., .4
            orientation: 'vertical'
            center: root.center

            TextInput:
                id: highscoreinput
                multiline: False
                size_hint_x: 0.5
                valign: "middle"
                halign: "center"
                hint_text: "Insert_your_name"
                pos_hint: { "center_x" : .5 }
                opacity: 0 if root.enterNewHighscore==False else 1
                on_text_validate: root.name_entered(self.text)

            SettingsBtn:
                text:"New Highscore"
                on_press:
                    root.generate_new_highscore()

            SettingsBtn:
                text:"back to menu"
                on_press:
                    root.manager.current = "Startscreen"

