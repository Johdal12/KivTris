<StartBtn@Button>:
    border: (0,0,0,0)
    size_hint_x: .5
    font_size: .2 * self.width if .2 * self.width < .5 * self.height else .5 * self.height
    background_normal: "Button_red6.png"
    background_down: "Button_down1.png"
    pos_hint: { "center_x" : .5 }

<GameBtn@StartBtn>:
    font_size: .15 * self.width if .15 * self.width < .4 * self.height else .4 * self.height

<GameBtnLbl@GameBtn>:
    background_down: "Button_red6.png"

<SettingsBtn@StartBtn>:
    font_size: .1 * self.width if .1 * self.width < .4 * self.height else .4 * self.height
    size_hint_x: 0.75

<CredBtn@Button>:
    font_size: .15 * self.width if .15 * self.width < .6 * self.height else .6 * self.height
    color: (0,0,0,1)
    size_hint: (.1, None)
    background_normal: "Button_yellow2.png"
    background_down: "Button_down1.png"
    pos_hint: { "center_y" : .5 }

<CredLbl@Label>:
    font_size: .5 * self.height
    text_size: ( self.parent.width*.9 , None )
    halign: "center"

<HighscoreLbl1@Label>:
    font_name: "cour.ttf"
    font_size: .33 * self.width if .33 * self.width < .9 * self.height else .9 * self.height
    text_size: ( self.parent.width*.9 , None )
    halign: "center"
    color: (1,1,0,1)
<HighscoreLbl2@HighscoreLbl1>:
    color: (0,1,1,1)

<Brick@Button>:
    #size: (12, 12)
    size_hint: (1.0/12.0, 1.0/22.0)
    allow_stretch: True

<Menu1>:
    name: "Startscreen"
    ## Startscreen
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.width, self.height/2
            source: "KivTris_back2.png"

    BoxLayout:
        padding: 10
        spacing: 20
        orientation: 'vertical'
        size: self.parent.size
        pos: self.parent.pos

        BoxLayout:
            size_hint: 1., .4
            orientation: 'vertical'
            center: root.center

            Button:
                pos_hint: { "center_x" : .5 }
                size_hint: .9 , .4
                allow_stretch: True
                background_normal: "KivTris_front2.png"
                background_down: "KivTris_front2.png"

        BoxLayout:
            orientation: 'vertical'
            center: root.center
            size_hint: 1., .6
            padding: 0
            spacing: 10

            StartBtn:
                text:"Credits"
                on_press:
                    root.manager.current = "Credits"
            StartBtn:
                text:"Game"
                on_press:
                    root.manager.current = "Game"
            StartBtn:
                text:"Highscore"
                on_press:
                    root.manager.current = "Highscore"
            StartBtn:
                text:"Quit"
                on_press:
                    root.quit_the_game()

<Menu3>:
    name: "Credits"
    ## Credits
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.width, self.height/2
            source: "KivTris_back2_dark.png"

    BoxLayout:
        padding: 10
        spacing: 20
        orientation: 'vertical'
        size: self.parent.size
        pos: self.parent.pos


        BoxLayout:
            size_hint: 1., .5
            orientation: 'vertical'
            center: root.center

            Label:
                text: "Credits"
                font_size: 24

            CredLbl:
                text:      root.scrollingText[ 0 + 3*root.offsetList]
                font_size: root.scrollingText[ 1 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[ 2 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[ 3 + 3*root.offsetList]
                font_size: root.scrollingText[ 4 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[ 5 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[ 6 + 3*root.offsetList]
                font_size: root.scrollingText[ 7 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[ 8 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[ 9 + 3*root.offsetList]
                font_size: root.scrollingText[10 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[11 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[12 + 3*root.offsetList]
                font_size: root.scrollingText[13 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[14 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[15 + 3*root.offsetList]
                font_size: root.scrollingText[16 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[17 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[18 + 3*root.offsetList]
                font_size: root.scrollingText[19 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[20 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[21 + 3*root.offsetList]
                font_size: root.scrollingText[22 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[23 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[24 + 3*root.offsetList]
                font_size: root.scrollingText[25 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[26 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[27 + 3*root.offsetList]
                font_size: root.scrollingText[28 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[29 + 3*root.offsetList]
            CredLbl:
                text:      root.scrollingText[30 + 3*root.offsetList]
                font_size: root.scrollingText[31 + 3*root.offsetList] + root.stdFontSize
                color:     root.scrollingText[32 + 3*root.offsetList]

        BoxLayout:
            size_hint: 1., .4
            orientation: 'horizontal'
            padding: 20
            spacing: 20

            CredBtn:
                text:"Font - ("+str(root.stdFontSize-1)+")"
                pos_hint_x: .25
                size_hint: .125, .25
                on_press:
                    root.reduce_std_font_size()
            CredBtn:
                text: ("Pause" if root.scrollingTextPaused == False else "Resume")
                pos_hint_x: .5
                size_hint: .125, .25
                on_press:
                    root.pause_scrolling_text()
            CredBtn:
                text:"Font + ("+str(root.stdFontSize+1)+")"
                pos_hint_x: .75
                size_hint: .125, .25
                on_press:
                    root.raise_std_font_size()

        BoxLayout:
            orientation: 'vertical'
            center: root.center
            size_hint: 1., .10
            padding: 0
            spacing: 10

            SettingsBtn:
                text:"back to menu"
                on_press:
                    root.manager.current = "Startscreen"

<Menu4>:
    name: "Game"
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.width, self.height/2
            source: "KivTris_back2_dark.png"

    ## full screen boxlayout
    BoxLayout:
        orientation: 'vertical'
        center: root.center
        size_hint: 1., 1.
        padding: 0
        spacing: 0

        ##top
        GridLayout:
            orientation: 'horizontal'
            size_hint: 1., .075
            cols: 2
            rows: 1
            padding: 0
            spacing: 0
            AnchorLayout:
                size_hint: .66, 1.
                anchor_x: "center"
                anchor_y: "center"
                Button:
                    border: (0,0,0,0)
                    size_hint: (None, 1.)
                    allow_stretch: False
                    background_normal: "KivTris_front2.png"
                    background_down: "KivTris_front2.png"
            AnchorLayout:
                size_hint: .34, 1.
                anchor_x: "center"
                anchor_y: "center"
                SettingsBtn:
                    border: (0,0,0,0)
                    size_hint_y: .8
                    text:"back to menu"
                    font_size:16
                    on_press:
                        root.manager.current = "Startscreen"

        ##middle = gamefield + info
        GridLayout:
            orientation: 'horizontal'
            size_hint_x: 1.
            padding: 0
            spacing: 0
            cols: 2

            AnchorLayout:
                size_hint_y: 1.0
                anchor_x: "center"
                anchor_y: "center"
                padding: 1

                GridLayout:
                    ## field size = 12 x 22 bricks
                    size_hint_y: 1.0 if (22.0 / 12.0) * (self.parent.width / self.parent.height) > 1.0 else (22.0 / 12.0) * (self.parent.width / self.parent.height)
                    size_hint_x: 1.0 if (12.0 / 22.0) * (self.parent.height / self.parent.width) > 1.0 else (12.0 / 22.0) * (self.parent.height / self.parent.width)
                    padding: 0
                    spacing: 0
                    rows: 22
                    cols: 12

